#set("script") {
    <!-- load compiled template simple.soy -->
    <script src="/soy/simple.js"></script>
}

#set("body") {
    <div id="helloName">
    </div>
}

#set("onload") {
    var payload = new Object();
    payload["name"] = '#(name)';

    var container = document.getElementById("helloName");
    container.innerHTML = soy.examples.simple.helloName(payload).content;
}

#embed("soy")
